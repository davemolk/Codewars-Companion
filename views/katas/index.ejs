<h2 class="centered display-3">Fave katas</h2>

<ul>
  <% katas.forEach(k => { %>
  <h3><%= k.name %> (id is <%= k.id %>)</h3>
  <a class="btn btn-secondary" href="/katas/<%= k.id %> ">more info</a>

  <a
    class="btn btn-secondary"
    target="_blank"
    href="http://www.codewars.com/kata/<%= k.cw %>/train/javascript"
    >train again</a
  >
  <a
    class="btn btn-secondary"
    target="_blank"
    href="http://www.codewars.com/kata/<%= k.cw %>/solutions/javascript"
    >solutions</a
  >

  <% }) %>
</ul>
